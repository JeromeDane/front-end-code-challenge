{
  "name": "frontend-code-challenge",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "min": "browserify public/src/index.js -d -p [minifyify --map index.min.map --output public/index.min.map]> public/index.min.js",
    "style": "lessc public/src/styles/ui.less --source-map=public/index.bundle.css.map --source-map-url=index.bundle.css.map > public/index.bundle.css",
    "style:min": "lessc public/src/styles/ui.less --source-map=public/index.bundle.css.map --source-map-url=index.bundle.css.map --clean-css=\"--compatibility=ie8 --advanced\" > public/index.bundle.css",
    "toc": "doctoc ."
  },
  "dependencies": {
    "backbone": "^1.2.1",
    "body-parser": "~1.8.1",
    "brfs": "^1.4.0",
    "browserify": "^11.0.1",
    "connect-browserify": "^4.0.0",
    "express": "~4.9.0",
    "ip": "^0.3.3",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "l10n.js": "git+https://github.com/JeromeDane/l10n.js.git",
    "lodash": "^2.4.1",
    "masonry-layout": "^3.3.1",
    "path": "^0.11.14",
    "random-seed": "^0.2.0",
    "raty": "git+https://github.com/wbotelhos/raty.git#2.7.0",
    "shim": "^0.6.0",
    "sly": "git+https://github.com/JeromeDane/sly.git",
    "stringify": "^3.1.0",
    "swipebox": "git+https://github.com/JeromeDane/swipebox.git",
    "tooltipster": "^1.0.9",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "doctoc": "^0.15.0",
    "less": "^2.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "minifyify": "^7.0.5"
  },
  "browser": "public/src/main.js",
  "browserify": {
    "transform": [
      "stringify"
    ]
  }
}
